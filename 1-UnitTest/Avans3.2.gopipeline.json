{
  "group": "defaultGroup",
  "name": "Avans3.2",
  "display_order_weight": -1.0,
  "label_template": "${COUNT}",
  "lock_behavior": "none",
  "environment_variables": [
    {
      "name": "nuget",
      "value": "C:\\Program Files\\MSBuild\\nuget.exe"
    }
  ],
  "parameters": [],
  "materials": [
    {
      "url": "https://github.com/AndreasFurster/Avans-3.2.git",
      "branch": "master",
      "shallow_clone": false,
      "filter": {
        "ignore": [],
        "whitelist": []
      },
      "auto_update": true,
      "type": "git"
    }
  ],
  "stages": [
    {
      "name": "Testing",
      "fetch_materials": true,
      "never_cleanup_artifacts": false,
      "clean_working_directory": false,
      "approval": {
        "type": "success",
        "users": [],
        "roles": [],
        "allow_only_on_success": false
      },
      "environment_variables": [],
      "jobs": [
        {
          "name": "run-unit-tests",
          "environment_variables": [],
          "tabs": [
            {
              "name": "CodeCoverage",
              "path": "testoutput/ReportGeneratorOutput/index.htm"
            }
          ],
          "resources": [],
          "artifacts": [
            {
              "source": "1-UnitTest/GeneratedReports/ReportGeneratorOutput",
              "destination": "testoutput",
              "type": "test"
            }
          ],
          "run_instance_count": "0",
          "timeout": 0.0,
          "tasks": [
            {
              "command": "dir",
              "timeout": -1.0,
              "arguments": [],
              "run_if": "passed",
              "type": "exec"
            },
            {
              "command": "dir",
              "working_directory": "1-UnitTest",
              "timeout": -1.0,
              "arguments": [],
              "run_if": "passed",
              "type": "exec"
            },
            {
              "command": "dotnet",
              "working_directory": "1-UnitTest",
              "timeout": -1.0,
              "arguments": [
                "clean",
                "UnitTesting.sln"
              ],
              "run_if": "passed",
              "type": "exec"
            },
            {
              "command": "C:\\Nuget\\nuget.exe",
              "working_directory": "1-UnitTest",
              "timeout": -1.0,
              "arguments": [
                "restore\r",
                "UnitTesting.sln\r",
                "-force\r",
                "-OutputDirectory\r",
                "packages"
              ],
              "run_if": "passed",
              "type": "exec"
            },
            {
              "command": "dotnet",
              "working_directory": "1-UnitTest",
              "timeout": -1.0,
              "arguments": [
                "build\r",
                "/p:Configuration\u003dDebug"
              ],
              "run_if": "passed",
              "type": "exec"
            },
            {
              "command": "packages\\OpenCover.4.7.922\\tools\\OpenCover.Console.exe",
              "working_directory": "1-UnitTest",
              "timeout": -1.0,
              "arguments": [
                "-target:packages\\NUnit.ConsoleRunner.3.10.0\\tools\\nunit3-console.exe\r",
                "-targetargs:UnitTesting\\bin\\Debug\\UnitTesting.dll\r",
                "-filter:\"+[*]UnitTesting.Program\"\r",
                "-mergebyhash\r",
                "-skipautoprops\r",
                "-register\r",
                "-output:GeneratedReports\\CoverageReport.xml"
              ],
              "run_if": "passed",
              "type": "exec"
            },
            {
              "command": "packages\\ReportGenerator.4.3.6\\tools\\net47\\ReportGenerator.exe",
              "working_directory": "1-UnitTest",
              "timeout": -1.0,
              "arguments": [
                "-reports:\"GeneratedReports\\CoverageReport.xml\"\r",
                "-targetdir:\"GeneratedReports\\ReportGeneratorOutput\"\r",
                "-reporttypes:HTML;Cobertura"
              ],
              "run_if": "passed",
              "type": "exec"
            }
          ]
        }
      ]
    }
  ]
}